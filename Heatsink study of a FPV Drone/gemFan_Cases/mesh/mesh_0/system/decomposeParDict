FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains  4;

method  scotch;

constraints
{
    faces
    {
        type    preserveFaceZones;
        zones ( air_to_heatSink );
        enabled true;
    }
}
